@model List<TelaCadastro.ViewModels.CidadeViewModel>
@{
    ViewBag.Title = "Cadastro - Cidade";
}

<div class="container">
    <div class="painel">
        <button type="button" class="botao botao-primario" onclick="window.location.href='@Url.Action("Incluir","Cidade")'" style="margin-bottom: 25px">
            Criar
        </button>
        <div id="divTabela">
            @Html.Partial("TabelaCidade", Model)
        </div>
    </div>
</div>
@section Scripts{
    <script>
        $(document).ready(function () {
            $("#tabelacidade").DataTable();
        })
    </script>
}



